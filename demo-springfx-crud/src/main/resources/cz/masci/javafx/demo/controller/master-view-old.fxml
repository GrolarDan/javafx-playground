<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <left>
    <VBox prefHeight="200.0" prefWidth="300.0" BorderPane.alignment="CENTER">
      <children>
        <Label fx:id="tableTitle" alignment="CENTER" prefWidth="300.0" text="List of items">
          <font>
            <Font name="Arial Bold Italic" size="14.0" />
          </font>
          <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
        </Label>
        <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
          <columns/>
          <VBox.margin>
            <Insets bottom="10.0" left="10.0" />
          </VBox.margin>
        </TableView>
      </children>
    </VBox>
  </left>
  <top>
    <Label fx:id="viewTitle" text="Editable" BorderPane.alignment="CENTER">
      <font>
        <Font name="Arial Black" size="24.0" />
      </font>
    </Label>
  </top>
  <center/>
</BorderPane>
